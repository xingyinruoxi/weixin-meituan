.list {
  background: #fff;
}
.list-item {
  padding: 20rpx 30rpx;
  border-bottom: 1px solid #ddd;
  overflow: hidden;
}
.list-img {
  float: left;
  width: 159rpx;
  height: 159rpx;
  border: 1px solid #eee;
}
.list-info {
  float: right;
  width: 509rpx;
}
.list-title {
  font: bold 30rpx/60rpx "宋体";
  color: #000;
}
.list-score {
  margin-top: 12rpx;
  float: left;
  width: 131rpx;
  height: 25rpx;
  overflow: hidden;
  background-image: url("data:image/png;base64,");
  background-size: auto 25rpx;
}
.list-score .score {
  width: 10%;
  background-image: url("data:image/png;base64,");
  background-size: auto 25rpx;
  height: 100%;
}
.list-price {
  float: left;
  margin-left: 10rpx;
  font-size: 26rpx;
  line-height: 52rpx;
  color: #000;
}
.list-position {
  position: relative;
  font-size: 26rpx;
  line-height: 52rpx;
  color: #999;
}
.list-dis {
  position: absolute;
  right: 0;
  top: 0;
}
.load-more {
  font-size: 30rpx;
  line-height: 60rpx;
  text-align: center;
  color: #333;
}